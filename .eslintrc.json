{
	"globals": {
		"EMOJIS": true,
		"PRODUCTION": true,
		"SCOPE_VERSION": true,
		"TRANSLATIONS": true,
		"oc_userconfig": true,
		"appName": true,
		"appVersion": true
	},
	"extends": [
		"@nextcloud/eslint-config/typescript",
		"plugin:storybook/recommended"
	],
	"plugins": [
		"cypress"
	],
	"parserOptions": {
		"babelOptions": {
			"plugins": [
				"@babel/plugin-syntax-import-assertions"
			]
		}
	},
	"rules": {
		"import/no-unresolved": [
			"error",
			{
				"ignore": ["\\?raw$"]
			}
		]
	}
}
